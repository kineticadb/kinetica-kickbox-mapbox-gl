{
  "name": "kinetica-kickbox-mapbox-gl",
  "version": "0.14.3",
  "description": "A library that facilitates interoperability between Kinetica and Mapbox.",
  "main": "src/index.js",
  "scripts": {
    "build:all": "npm run test && npm run clean:all && npm run build:dev && npm run build:prod",
    "build:prod": "npm run test && npm run clean:prod && npm run webpack:prod && npm run webpack:umd && npm run sass:prod",
    "build:dev": "npm run clean:dev && npm run webpack:dev && npm run sass:dev",
    "build:test": "npx webpack --config build/webpack.config.test.js",
    "clean:all": "rm -rf test-output && rm -rf dist && rm -rf dev-dist",
    "clean:dev": "rm -rf dev-dist",
    "clean:prod": "rm -rf prod",
    "lint": "",
    "sass:prod": "./node_modules/node-sass/bin/node-sass --output-style compressed --importer node_modules/node-sass-package-importer/dist/cli.js -o dist src/style/kickbox.scss",
    "sass:dev": "./node_modules/node-sass/bin/node-sass --source-map true --importer node_modules/node-sass-package-importer/dist/cli.js -o dev-dist src/style/kickbox.scss",
    "test": "npm run build:test && mocha ./test/*_spec.js --opts ./test/mocha.opts",
    "test:debug": "npm run build:test && mocha --inspect-brk",
    "webpack:prod": "npx webpack --config build/webpack.config.js",
    "webpack:umd": "npx webpack --config build/webpack.config.umd.js",
    "webpack:dev": "npx webpack --config build/webpack.config.development.js",
    "watch:dev": "nodemon --watch ./src -e js,handlebars,scss -x \"npm run build:dev\""
  },
  "keywords": [
    "Kinetica",
    "Mapbox",
    "Library"
  ],
  "author": "Matthew Brown",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "babel-loader": "^8.0.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "handlebars-loader": "^1.6.0",
    "html-loader": "^0.5.5",
    "jsdom": "^11.6.2",
    "karma": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.0.1",
    "mocha-phantomjs": "^4.1.0",
    "node-sass": "^7.0.0",
    "node-sass-package-importer": "^5.1.0",
    "sass-loader": "^6.0.7",
    "testdouble": "^3.5.2",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.1.5"
  },
  "dependencies": {
    "@turf/circle": "^6.0.0",
    "@turf/helpers": "^6.0.1",
    "@turf/length": "^6.0.0",
    "axios": "^0.18.0",
    "handlebars": "^4.0.11",
    "lodash": "^4.17.5",
    "spin.js": "^3.1.0",
    "supercluster": "^3.0.2",
    "uuid": "^3.2.1"
  }
}
